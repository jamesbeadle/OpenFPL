{
  "name": "open_fpl_frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Open FPL Game",
  "keywords": [
    "Open FPL",
    "FPL",
    "IC",
    "ICP"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prebuild": "npm run generate",
    "generate": "dfx generate && node ./update.types.mjs && npm run format"
  },
  "devDependencies": {
		"@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.1",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "prettier": "^2.6.2",
    "prettier-plugin-motoko": "^0.2.3",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.69.5",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^0.19.3",
    "@dfinity/auth-client": "^0.19.3",
    "@dfinity/candid": "^0.19.3",
    "@dfinity/gix-components": "^3.0.0",
    "@dfinity/principal": "^0.19.3",
    "@dfinity/utils": "^1.0.0",
    "postcss": "^8.4.31",
    "svelte-flag-icons": "^0.7.2",
    "tailwindcss": "^3.3.5"
  }
}
